# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'WhatsON' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for WhatsON
pod ‘Firebase/Core’
pod ‘Firebase/Auth’
pod ‘Firebase/Database’
pod ‘Firebase/Storage’
pod 'FBSDKLoginKit'
pod 'FBSDKCoreKit'
pod 'FacebookCore'
pod 'FacebookLogin'
pod 'FacebookShare'
pod 'GeoFire', '~> 1.1'

end
